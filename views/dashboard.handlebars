{{! Showing blog posts for specific user to update }}

{{#each users as |user|}}
  <div class="row align-center mb-5">
    <div class="col-md-6">
      <h2>{{{get_emoji}}} {{user.name}}</h2>
    </div>
    <div class="col-md-6">
      <h3><a href="mailto:{{user.email}}">{{user.email}}</a>Test</h3>
    </div>
  </div>
{{/each}}